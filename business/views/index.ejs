          <!-- 添加商品 -->
          <div class="add-product">
            <form>
              <!-- 商品类型 -->
              <div class="form-group">
                <label for="product-type">商品类型</label>
                <select id="product-type" class="form-control" name="product-type">
                  <option selected>选择商品类型</option>
                </select>
                <div class="invalid-feedback"></div>
              </div>

              <!-- 商品名称 -->
              <div class="form-group">
                <label for="product-name">商品名称</label>
                <input type="text" class="form-control" id="product-name" autocomplete="off" placeholder="输入商品名称" name="product-name" />
                <div class="invalid-feedback"></div>
              </div>

              <!-- 商品价格 -->
              <div class="form-group">
                <label for="product-price">商品价格</label>
                <input type="text" class="form-control" id="product-price" autocomplete="off" name="product-price" placeholder="输入商品价格" />
                <div class="invalid-feedback"></div>
              </div>

              <!-- 会员价格 -->
              <div class="form-group">
                <label for="vip-price">会员价格</label>
                <input type="text" class="form-control" id="vip-price" autocomplete="off" placeholder="输入会员价格" name="vip-price">
                <div class="invalid-feedback"></div>
              </div>

              <!-- <div class="form-group form-check">
                <input type="checkbox" class="form-check-input" id="open-vip">
                <label class="form-check-label" for="exampleCheck1">是否开启会员价格</label>
              </div> -->

              <div class="form-group">
                <!-- <input type="checkbox" class="form-check-input" id="open-vip">
                <label class="form-check-label" for="open-vip">是否开启会员价格</label> -->

                <div class="custom-control custom-checkbox">
                  <input type="checkbox" class="custom-control-input" id="open-vip">
                  <label class="custom-control-label" for="open-vip">是否开启会员价格</label>
                </div>
              </div>

              <!-- 商品详情描述 -->
              <div class="form-group">
                <label for="product-details">商品详情描述</label>
                <input type="text" class="form-control" id="product-detail" autocomplete="off" placeholder="商品详情描述" name="product-detail">
                <div class="invalid-feedback"></div>
              </div>

              <!-- 商品库存 -->
              <div class="form-group">
                <label for="product-count">库存量</label>
                <input type="text" class="form-control" id="product-count" autocomplete="off" placeholder="库存量" name="product-count">
                <div class="invalid-feedback"></div>
              </div>

              <!-- 商品图片 -->
              <div class="form-group">
                <label class="upload">
                  上传商品图片
                  <input type="file" id="upload" class="form-control-file" name="product-img">
                  <div class="invalid-feedback">请选择上传商品图片</div>
                </label>
              </div>

              <div class="form-group preview-box">
                <div id="preview-img" class="preview-img"></div>
              </div>

              <button id="commit" type="button" class="btn btn-primary">提交</button>
            </form>
          </div>

          
          <!-- 编辑商品 -->
          <div class="edit-product">
            <table class="table table-bordered text-center table-hover table-sm">
              <thead>
                <tr>
                  <th>
                    <input type="checkbox">
                  </th>
                  <th>序号</th>
                  <th>商品图片</th>
                  <th>商品id</th>
                  <th>商品名称</th>
                  <th>商品价格</th>
                  <th>vip价格</th>
                  <th>是否开启vip价格</th>
                  <th>商品类型</th>
                  <th>库存量</th>
                  <th>上架日期</th>
                  <th>操作</th>
                </tr>
              </thead>
              <tbody>
                
              </tbody>
            </table>
            <nav aria-label="Page navigation example">
